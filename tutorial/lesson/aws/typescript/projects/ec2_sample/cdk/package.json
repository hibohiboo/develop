    "jest": "^27.2.5",
    "npm-check-updates": "^11.8.5",
    "ts-node": "^10.3.0",
    "typescript": "~4.4.4"
  },
  "dependencies": {
    "@aws-cdk/aws-ec2": "^1.128.0",
    "@aws-cdk/aws-lambda": "^1.128.0",
    "@aws-cdk/core": "^1.128.0",
